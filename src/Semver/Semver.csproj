<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Acklann.Semver</AssemblyName>
    <RootNamespace>Acklann.Semver</RootNamespace>
    <LangVersion>7.2</LangVersion>
  </PropertyGroup>
  <PropertyGroup>
    <Company>Ackara</Company>
    <PackageProjectUrl>https://github.com/Ackara/Semver</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Ackara/Semver.git</RepositoryUrl>
    <Authors>Ackara</Authors>
    <PackageTags>semantic version</PackageTags>
  </PropertyGroup>
  <PropertyGroup>
    <Copyright>Ackara Copyright 2019</Copyright>
    <AssemblyVersion>0.0.3</AssemblyVersion>
    <PackageVersion>0.0.3</PackageVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <DocumentationFile>$(OutputPath)$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Memory" Version="4.5.3" />
  </ItemGroup>
</Project>
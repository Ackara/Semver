<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Acklann.Semver</AssemblyName>
    <RootNamespace>Acklann.Semver</RootNamespace>
    <LangVersion>7.2</LangVersion>
  </PropertyGroup>
  <PropertyGroup Label="Package">
    <PackageVersion>0.0.7</PackageVersion>
    <AssemblyVersion>0.0.7</AssemblyVersion>
    <Description>A comprehensive implementation of Semantic Version (https://semver.org/).</Description>
    <Company>Ackara</Company>
    <Authors>Ackara</Authors>
    <Copyright>Copyright Ackara 2019</Copyright>
    <PackageIconUrl>https://raw.githubusercontent.com/Ackara/Semver/master/art/semver.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/Ackara/Semver</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Ackara/Semver.git</RepositoryUrl>
    <PackageTags>semantic version</PackageTags>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <DocumentationFile>$(OutputPath)$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Memory" Version="4.5.3" />
  </ItemGroup>
</Project>